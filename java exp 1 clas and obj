// File name: Box.java
class Box {
    double width;
    double height;
    double depth;

    // Constructor
    Box() {
        width = 10;
        height = 20;
        depth = 15;
    }

    void volume() {
        double vol = width * height * depth;
        System.out.println("Volume is " + vol);
    }

    public static void main(String[] args) {
        Box mybox = new Box();
        mybox.volume();
    }
}
